# -*- Shell-Script -*-

export PS1='$ ';
export PS3='? ';

set -o ignoreeof
shopt -s cdspell

function +()  { pushd "$@"; }
function -()  { popd "$@"; }
function =()  { dirs -v "$@"; }
function j()  { jobs "$@"; }
function k()  { kill -9 "$@"; }
function l()  { ls -Alhrt "$@"; }

[ -x /bin/uname ] && {
    case $(/bin/uname -o) in
    Msys)
	. $HOME/.bash_Msys
	;;
    esac
}

# vim: filetype=sh:
##end .bash_interactive
