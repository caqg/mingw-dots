# -*- Shell-Script -*-

export PATH="\
$HOME/cmd\
:$HOME/bin\
:/c/MinGW/bin\
:/c/MinGW/mingw32/bin\
:/c/MinGW/msys/1.0/bin\
:/c/Program Files (x86)/Vim/vim74\
:/c/Program Files (x86)/Emacs/emacs-24.3/bin\
:/c/Program Files (x86)/clisp-2.49\
:/c/Program Files/swipl/bin\
:/c/Program Files (x86)/Git/bin\
:/c/Program Files (x86)/WinMerge\
:/c/Python34\
:/c/Python34/Scripts\
:/c/Python34/Tools/Scripts\
:/c/Ruby193/bin\
:/c/Windows/system32\
:/c/Windows\
:/c/Windows/System32/Wbem\
:/c/Windows/System32/WindowsPowerShell/v1.0/\
:/c/Program Files/TortoiseGit/bin\
:/c/Program Files/TortoiseSVN/bin\
:.\
"

# This setting interacts poorly with Emacs 24.3.  Unsetting it allows Emacs to
# find man pages!
#
# export MANPATH="\
# $HOME/share/man\
# :/c/MinGw/msys/1.0/share/man\
# :/c/MinGw/share/man\
# "

export INFOPATH="\
$HOME/share/info\
:/c/MinGW/msys/1.0/share/info\
:/c/MinGW/share/info\
:/c/Program Files (x86)/Emacs/emacs-24.3/info\
"

PERL5LIB=$HOME/lib/perl5
export PERL5LIB

export CVSROOT=:local:$HOME/src/cvsroot

export PAGER=less
export LESS=isrRQ
export EDITOR=$HOME/cmd/e
export VISUAL=$HOME/cmd/e

# export TZ=:America/Los_Angeles

##end .bash_env
